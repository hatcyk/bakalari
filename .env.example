# Bakaláři API Cookie
# Your authentication cookie from bakalari.gyarab.cz
BAKALARI_COOKIE='your_cookie_here'

# Firebase Service Account
# Path to your Firebase service account JSON file
FIREBASE_SERVICE_ACCOUNT_PATH='./firebase-service-account.json'

# Debug mode - enables test endpoints and debug features
DEBUG=false

# Skip initial prefetch on server startup (useful for development)
# Set to 'true' to skip the automatic prefetch when server starts
# You can still trigger prefetch manually via: POST /api/prefetch/trigger
SKIP_INITIAL_PREFETCH=false

# Cron Secret (for Vercel Cron Jobs)
# Generate a random secret: openssl rand -base64 32
# Set this in Vercel Environment Variables
CRON_SECRET='your_random_secret_here'
